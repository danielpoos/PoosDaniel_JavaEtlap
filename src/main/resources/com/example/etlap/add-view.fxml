<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.etlap.AddController">
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="180.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints maxWidth="150.0" minWidth="10.0" prefWidth="99.4" />
      <ColumnConstraints maxWidth="207.4" minWidth="10.0" prefWidth="201.4" />
   </columnConstraints>
   <children>
      <TextField fx:id="textName" prefHeight="26.0" prefWidth="200.0" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets right="5.0" />
         </GridPane.margin></TextField>
      <Spinner fx:id="scrollPrice" amountToStepBy="50" initialValue="500" max="15000" min="50" prefHeight="26.0" prefWidth="195.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#addOnClick" prefWidth="290.0" text="Add" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER" />
      <ChoiceBox fx:id="choiceCategory" prefHeight="26.0" prefWidth="195.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="Name" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
         <padding>
            <Insets right="5.0" />
         </padding></Label>
      <Label text="Details" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <padding>
            <Insets right="5.0" />
         </padding></Label>
      <Label text="Category" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
         <padding>
            <Insets right="5.0" />
         </padding></Label>
      <Label text="Price" GridPane.halignment="RIGHT" GridPane.rowIndex="3" GridPane.valignment="CENTER">
         <padding>
            <Insets right="5.0" />
         </padding></Label>
      <TextArea fx:id="textDetail" prefHeight="170.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
</GridPane>
