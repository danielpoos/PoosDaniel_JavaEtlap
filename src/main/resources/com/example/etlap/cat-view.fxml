<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.etlap.CatController">
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints maxWidth="150.0" minWidth="10.0" prefWidth="99.4" />
      <ColumnConstraints maxWidth="207.4" minWidth="10.0" prefWidth="201.4" />
   </columnConstraints>
   <children>
      <TextField fx:id="textName" prefHeight="26.0" prefWidth="200.0" GridPane.columnIndex="1">
         <GridPane.margin>
            <Insets right="5.0" />
         </GridPane.margin></TextField>
      <Button alignment="CENTER" mnemonicParsing="false" onAction="#addOnClick" prefWidth="290.0" text="Add" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
      <Label text="Name" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
         <padding>
            <Insets right="5.0" />
         </padding></Label>
   </children>
</GridPane>
